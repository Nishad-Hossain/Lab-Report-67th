#include <stdio.h>

int main() {
    int n, i;
    
    // Input size of the array
    printf("Enter the number of elements in the array: ");
    scanf("%d", &n);
    
    int arr1[n], arr2[n];
    
    // Input elements of the first array
    printf("Enter elements of the first array:\n");
    for(i = 0; i < n; i++) {
        scanf("%d", &arr1[i]);
    }
    
    // Copy elements to the second array
    for(i = 0; i < n; i++) {
        arr2[i] = arr1[i];
    }
    
    // Print the second array
    printf("Elements of the second array (copied from first array):\n");
    for(i = 0; i < n; i++) {
        printf("%d ", arr2[i]);
    }
    
    return 0;
}
