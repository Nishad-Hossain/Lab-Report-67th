#include <stdio.h>
#include <string.h>

int main() {
    char str[200];
    char delimiter[10];

    printf("Enter the string: ");
    gets(str);

    printf("Enter the delimiter (e.g., space, comma): ");
    gets(delimiter);

    // Use strtok to split the string
    char *token = strtok(str, delimiter);

    printf("Substrings:\n");
    while (token != NULL) {
        printf("%s\n", token);
        token = strtok(NULL, delimiter);
    }

    return 0;
}
