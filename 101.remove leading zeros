#include <stdio.h>
#include <string.h>

int main() {
    char str[100];

    printf("Enter a numeric string: ");
    gets(str);

    int i = 0;
    // Skip all leading zeros
    while (str[i] == '0') {
        i++;
    }

    // If the string was all zeros
    if (str[i] == '\0') {
        printf("0\n");
    } else {
        printf("String without leading zeros: %s\n", &str[i]);
    }

    return 0;
}

