#include <stdio.h>
#include <string.h>

int main() {
    int n, i, j;
    char temp[100];

    printf("Enter number of strings: ");
    scanf("%d", &n);

    char str[n][100];

    // Input Strings
    printf("Enter %d strings:\n", n);
    for(i = 0; i < n; i++) {
        scanf("%s", str[i]);
    }

    // Sorting strings (Bubble sort logic)
    for(i = 0; i < n - 1; i++) {
        for(j = i + 1; j < n; j++) {
            if(strcmp(str[i], str[j]) > 0) {
                strcpy(temp, str[i]);
                strcpy(str[i], str[j]);
                strcpy(str[j], temp);
            }
        }
    }

    // Output sorted strings
    printf("Sorted strings:\n");
    for(i = 0; i < n; i++) {
        printf("%s\n", str[i]);
    }

    return 0;
}
