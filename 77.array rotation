#include <stdio.h>

int main() {
    int n, i, j, k, temp;
    
    // Input size of array
    printf("Enter the number of elements in the array: ");
    scanf("%d", &n);
    
    int arr[n];
    printf("Enter the elements of the array:\n");
    for(i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }
    
    // Input number of positions to rotate
    printf("Enter number of positions to rotate: ");
    scanf("%d", &k);
    
    // Right rotation
    k = k % n; // in case k > n
    for(i = 0; i < k; i++) {
        temp = arr[n-1]; // store last element
        for(j = n-1; j > 0; j--) {
            arr[j] = arr[j-1]; // shift elements right
        }
        arr[0] = temp; // put last element at first position
    }
    
    // Print rotated array
    printf("Array after rotation:\n");
    for(i = 0; i < n; i++) {
        printf("%d ",
