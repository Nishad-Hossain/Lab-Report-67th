#include <stdio.h>
#include <math.h>

int main() {
    int lower, upper, num, originalNum, remainder, n;
    double sum;

    // Input interval
    printf("Enter two numbers (lower and upper): ");
    scanf("%d %d", &lower, &upper);

    printf("Armstrong numbers between %d and %d are:\n", lower, upper);

    for (num = lower; num <= upper; num++) {
        originalNum = num;
        sum = 0;

        // Count number of digits
        n = 0;
        while (originalNum != 0) {
            originalNum /= 10;
            n++;
        }

        originalNum = num;

        //
